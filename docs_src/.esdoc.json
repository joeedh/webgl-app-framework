{
  "source": "../scripts",
  "excludes": [
    ".*extern.*"
  ],
  "outputAST" : false,
  "destination": "../docs",
  "index": "../docs/index.html",
  "plugins": [
    {
      "name": "esdoc-ecmascript-proposal-plugin", 
      "option": {
      "all": true
      }
    },{
    "name": "esdoc-standard-plugin",
    "option": {
      "coverage": {"enable": true},
      "lint": {"lint" : false},
      "accessor": {"access": ["public", "protected", "private"], "autoPrivate": true},
      "undocumentIdentifier": {"enable": true},
      "unexportedIdentifier": {"enable": true},
      "typeInference": {"enable": true},
      "brand": {
        "title": "path.ux",
        "description": "this is awesome library",
        "repository": "https://github.com/joeedh/path.ux.git",
        "site": "https://github.com/joeedh/path.ux",
        "author": "Joseph Eagar"
      },
      "manual": {
        "index" : "index.md",
        "globalIndex" : true,
        "files": [
          "../scripts/path.ux/docs/docs_src/architecture.md",
          "../scripts/path.ux/docs/docs_src/nstructjs.md",
          "../scripts/path.ux/docs/docs_src/toolsystem.md",
          "../scripts/path.ux/docs/docs_src/controller.md",
          "datablock.md",
          "datagraph.md",
          "../scripts/path.ux/docs/docs_src/toolprop.md",
          "../scripts/path.ux/docs/docs_src/tutorial.md",
          "../scripts/path.ux/docs/docs_src/context.md",
          "toolmode.md",
          "mycontext.md",
          "widgets.md",
          "view3d.md"
        ]
      }
    }
  }]
}

